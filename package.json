    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.5.2",
    "systemjs": "^0.19.39",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "yargs": "^5.0.0",
