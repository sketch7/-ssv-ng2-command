{
  "name": "@ssv/ngx.command",
  "version": "0.2.0",
  "description": "Command pattern implementation for angular. Command used to encapsulate information which is needed to perform an action.",
  "keywords": [
    "sketch7",
    "ngx",
    "angular2",
    "angular",
    "ssv",
    "command",
    "pattern",
    "ui"
  ],
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "test-devt": "devtool ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "clean": "rimraf dist",
    "lint": "gulp lint",
    "build": "ng-packagr -p ng-package.json",
    "prepare-release": "gulp prepare-release"
  },
  "author": "Stephen Lautier <stephen.lautier@outlook.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sketch7/ssv-ng2-command.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/core": "^5.0.0",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/compiler-cli": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@ssv/tools": "^0.6.12",
    "@types/jest": "^21.1.8",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.8.0",
    "gulp-conventional-changelog": "^1.1.7",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "ng-packagr": "2.0.0-rc.5",
    "reflect-metadata": "^0.1.10",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "rxjs": "^5.5.2",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "yargs": "^10.0.3",
    "zone.js": "^0.8.18"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
